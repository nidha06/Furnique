    <meta name="description" content="About Furnique - Premium furniture crafted with excellence" />
    <meta name="keywords" content="furniture, home decor, quality furniture, about us" />
  
    <!-- Bootstrap CSS -->
    <link href="/css/tiny-slider.css" rel="stylesheet">
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css" rel="stylesheet">
    <link href="/css/bootstrap.min.css" rel="stylesheet">
    <link href="/css/style.css" rel="stylesheet">
    <title>FURNIQUE | Where Luxury Meets Function</title>
    <style>
        .about-hero {
            background-color: #f8f9fa;
            padding: 80px 0;
            margin-bottom: 50px;
        }
        
        .about-hero h1 {
            font-size: 2.5rem;
            font-weight: 700;
            margin-bottom: 20px;
        }
        
        .about-section {
            padding: 60px 0;
        }
        
        .about-img {
            border-radius: 10px;
            overflow: hidden;
            box-shadow: 0 5px 15px rgba(0,0,0,0.1);
        }
        
        .timeline {
            position: relative;
            padding: 40px 0;
        }
        
        .timeline:before {
            content: '';
            position: absolute;
            height: 100%;
            width: 2px;
            background-color: #ddd;
            left: 50%;
            transform: translateX(-50%);
        }
        
        .timeline-item {
            margin-bottom: 40px;
            position: relative;
        }
        
        .timeline-content {
            padding: 20px;
            background-color: #fff;
            border-radius: 5px;
            box-shadow: 0 3px 10px rgba(0,0,0,0.1);
        }
        
        .year-badge {
            background-color: #3b5d50;
            color: white;
            padding: 8px 16px;
            border-radius: 30px;
            display: inline-block;
            margin-bottom: 15px;
            font-weight: 600;
        }
        
        .team-member {
            text-align: center;
            margin-bottom: 30px;
        }
        
        .team-img {
            width: 180px;
            height: 180px;
            border-radius: 50%;
            overflow: hidden;
            margin: 0 auto 20px;
            box-shadow: 0 5px 15px rgba(0,0,0,0.1);
        }
        
        .team-img img {
            width: 100%;
            height: 100%;
            object-fit: cover;
        }
        
        .values-card {
            padding: 30px;
            border-radius: 10px;
            background-color: #f8f9fa;
            height: 100%;
            transition: all 0.3s;
        }
        
        .values-card:hover {
            transform: translateY(-5px);
            box-shadow: 0 10px 20px rgba(0,0,0,0.1);
        }
        
        .values-icon {
            font-size: 2.5rem;
            margin-bottom: 20px;
            color: #3b5d50;
        }
        
        @media (max-width: 767px) {
            .about-hero {
                padding: 50px 0;
            }
            
            .timeline:before {
                left: 30px;
            }
            
            .team-img {
                width: 150px;
                height: 150px;
            }
        }
    </style>
</head>
